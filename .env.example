# AI Provider API Keys
# Configure at least one provider for the AI Gateway to function

# OpenAI (GPT-4, GPT-4 Turbo, GPT-3.5)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic (Claude 3.5 Sonnet, Claude 3 Opus, Claude 3 Haiku)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini (Gemini 2.5 Pro, Gemini 2.5 Flash)
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Legacy Google Gemini API Key (for backwards compatibility)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Turso Database Configuration
VITE_TURSO_DATABASE_URL=your_turso_database_url_here
VITE_TURSO_AUTH_TOKEN=your_turso_auth_token_here

# AI Gateway Configuration
# Default provider: openai, anthropic, or google
VITE_DEFAULT_AI_PROVIDER=google
# Default model (e.g., "openai:gpt-4o", "anthropic:claude-3-5-sonnet-20241022", "google:gemini-2.5-flash")
VITE_DEFAULT_AI_MODEL=google:gemini-2.5-flash

# Cost and Usage Limits
VITE_MONTHLY_AI_BUDGET=50
VITE_ENABLE_AUTO_FALLBACK=true
VITE_ENABLE_COST_OPTIMIZATION=true
